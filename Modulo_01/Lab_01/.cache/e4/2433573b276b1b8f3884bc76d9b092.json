n\r\n    @for $i from 1 to 5 {\r\n        .darken-#{$i} {\r\n            background-color: darken($base-color, $i*7%);\r\n        }\r\n    \r\n        .lighten-#{$i} {\r\n            background-color: lighten($base-color, $i*7%);\r\n        }\r\n    }\r\n}\r\n\r\n\r\n.violet {\r\n    $base-color: purple;\r\n\r\n    .base-color {\r\n        background-color: $base-color;\r\n    }\r\n\r\n    @for $i from 1 to 5 {\r\n        .darken-#{$i} {\r\n            background-color: darken($base-color, $i*5%);\r\n        }\r\n    \r\n        .lighten-#{$i} {\r\n            background-color: lighten($base-color, $i*5%);\r\n        }\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"7ae0c93678750a405440f698d9114f0b","cacheData":{"env":{}}}